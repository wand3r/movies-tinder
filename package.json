{
  "name": "tinder-for-movies",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.ts",
  "scripts": {
    "clean": "rimraf ./build/**.*",
    "compile-webpack-config": "tsc --allowSyntheticDefaultImports --skipLibCheck webpack.config.ts",
    "build-development": "yarn run clean && yarn run compile-webpack-config && webpack --env.environment development",
    "build-production": "yarn run clean && yarn run compile-webpack-config && webpack --env.environment production",
    "build-production-analyze": "yarn run clean && yarn run compile-webpack-config && webpack --env.environment production --env.analyze",
    "dev-server": "yarn run compile-webpack-config && webpack-dev-server --env.environment development",
    "test": "jest",
    "test-watch": "jest --watch",
    "storybook": "start-storybook -p 9001 -c .storybook  -s ./static"
  },
